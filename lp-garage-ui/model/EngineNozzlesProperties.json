{
    "inputParameters":{
        "pretendedGasSpeedinAdmissionValve":{
            "alias":"pgsav",
            "units":"m/s",
            "value":null
        },
        "admissionValvesDiameter":{
            "alias":"avD",
            "units":"mm",
            "value": null
        },
        "cylinderStroke":{
            "alias":"L",
            "units":"mm",
            "value":null
        },
        "camshaftAdmissionOpen":{
            "alias":"cAO",
            "units":"deg",
            "value":null
        },
        "camshaftAdmissionClose":{
            "alias":"cAC",
            "units":"deg",
            "value":null
        },
        "camshaftEscapeOpen":{
            "alias":"cEO",
            "units":"deg",
            "value":null
        },
        "camshaftEscapeClose":{
            "alias":"cEC",
            "units":"deg",
            "value":null
        }
    },
    "formulas":{
        "admissionvalvediameter":{
            "alias":"Dv",
            "formula":"(sqrt(((Vh/1cm^3)*n)/(pi*pgsav*750s/m)))*10 mm"
        },
        "avaragepistonspeed":{
            "alias":"avps",
            "formula":"((L in m)*n)/30s"
        },
        "firstductdiameter":{
            "alias":"fdd",
            "formula":"Dv"
        },
        "firstductsection":{
            "alias":"fds",
            "formula":"((fdd^2)*pi)/4"
        },
        "gasspeedinfirstduct":{
            "alias":"gsfd",
            "formula":"avps*(AP/fds)"
        },
        "secondductdiameter":{
            "alias":"sdd",
            "formula":"Dv*0.8"
        },
        "seconductsection":{
            "alias":"sds",
            "formula":"((sdd^2)*pi)/4"
        },
        "gasspeedinsecondduct":{
            "alias":"gssd",
            "formula":"avps*(AP/sds)"
        },
        "valveductdiameter":{
            "alias":"vdd",
            "formula":"Dv*0.9"
        },
        "valveductsection":{
            "alias":"vds",
            "formula":"((vdd^2)*pi)/4"
        },
        "valvesection":{
            "alias":"vs",
            "formula":"((avD^2)*pi)/4"
        },
        "gasspeedinvalveduct":{
            "alias":"gsvd",
            "formula":"avps*(AP/(vds-vs))"
        },
        "admissionvalvehigh":{
            "alias":"avh",
            "formula":"(avps*AP)/(pgsav*pi*Dv*(cos(30 deg))) in mm"
        },
        "admissionopenedsection":{
            "alias":"aos",
            "formula":"pi*Dv*avh*(cos(30 deg)) in mm^2"
        },
        "gasspeedinvalve":{
            "alias":"gsv",
            "formula":"avps*(AP/aos)"
        },
        "admissionductlongwithrunners":{
            "alias":"adl",
            "formula": "(((2*(fds/100)*(RC-1))/((((n*2)/(955*343))^2)*(Vh)*(RC+1)))*1cm/m^-1)in cm"
        },
        "exhaustvalvediameter":{
            "alias":"evd",
            "formula": "Dv*0.9"
        },
        "exhaustfirstsectioncolectorlong":{
            "alias":"efcl",
            "formula":"((13000*(cEO+cEC+180deg))/(1deg)/(n*6))*(1cm)"
        },
        "exhaustfirstsectiondiameter":{
            "alias":"efd",
            "formula":"2*sqrt(((VH/4)*2)/(efcl*pi)) in cm"
        },
        "exhaustfirstsectiondiametercurvecorrection":{
            "alias":"efdC",
            "formula":"efd+(efd*0.1)"
        },
        "exhaustsecondarysectiondiameter":{
            "alias":"esd",
            "formula":"2*sqrt(VH/(efcl*pi))"
        },
        "exhaustsecondarysectiondiametercurvecorrection":{
            "alias":"esdC",
            "formula":"esd+(esd*0.1)"
        }
    }
}