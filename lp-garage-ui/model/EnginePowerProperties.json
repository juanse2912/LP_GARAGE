{
    "inputParameters": {
        "intakeAirPressure": {
            "alias":"PA",
            "units":"kPa",
            "value": null
        },
        "intakeAirTemperature":{
            "alias":"TA",
            "units":"K",
            "value":null
        }
    },
    "formulas":{
        "airDensity": {
            "alias":"pA",
            "formula":"PA/(CA*TA)"
        },
        "compressionPressure":{
            "alias":"PC",
            "formula":"(PA*(Vh+VC)^k)/VC^k"
        },
        "compressionTemperature":{
            "alias":"TC",
            "formula":"(TA*(Vh+VC)^(k-1))/VC^(k-1)"
        },
        "airMass":{
            "alias":"ma",
            "formula":"((nV*Vh*n*pA)/200)/60s"
        },
        "fuelMass":{
            "alias":"mc",
            "formula":"ma/afr"
        },
        "contributedHeat":{
            "alias":"Qap",
            "formula":"mc*LHV"
        },
        "combustionTemperature":{
            "alias":"Tz",
            "formula":"(1 K)*(Qap/(cev*ma)) + TC"
        },
        "combustionPressure":{
            "alias":"Pz",
            "formula":"(Tz*PC)/TC"
        },
        "pressureDifferential":{
            "alias":"l1",
            "formula":"Pz/PC"
        },
        "indicatedmediumpressure":{
            "alias":"pmi",
            "formula":"PA*((RC^n1)/(RC-1))*((l1/(n2-1))*(1-(1/(RC^(n2-1))))-(1/(n1-1))*(1-(1/(RC^(n1-1)))))"
        },
        "indicatedpower":{
            "alias":"pi",
            "formula":"((pmi/ 9.81 in N)*Vh*n)/(2*60s*75kg) * (1 in hp)"
        },
        "expansionpressure":{
            "alias":"Pb",
            "formula":"(Pz*VC^k)/(Vh+VC)^k" 
        },
        "pressureDifferential2":{
            "alias":"l2",
            "formula":"Pz/Pb" 
        },
        "effectivemediumpressure":{
            "alias":"pme",
            "formula":"PA*((((RC^(k-1))*(l2-1)))/((RC-1)(k-1)))*(1-(1/(RC^(k-1))))"
        },
        "efectivePower":{
            "alias":"pe",
            "formula":"((pme)*(VH in m^3)*n)/((75kg)*120 s)*(1 in hp)"
        }
    }
}