
{
    "inputParameters": {
        "intendedCompressionRatio": {
            "alias":"ic",
            "value": null
        },
        "standardCylinderDiameter":{
            "alias":"Stcd",
            "units":"mm",
            "value":null

        },
        "cylinderTopwWear":{
            "alias":"cw",
            "units":"mm",
            "value":null

        },
        "standardCrankJornalDiameter": {
            "alias":"cd",
            "units":"mm",
            "value": null
        },
        "crankTopWear":{
            "alias":"cp",
            "units":"mm",
            "value":null
        }
    },
    "formulas":{

        "thicknesstobedevastated": {
            "alias":"td",
            "formula":"(L/(RC-1))-(L/(ic-1))"
        },
        "cylinderrectification":{
            "alias":"cr",
            "formula":"floor(Stcd-cw,2)+.01 in"

        },
        "diameterrectifiedcylinder":{
            "alias":"drc",
            "formula":"cr + cw"

        },
        "cylinderdiameterbeforehoning":{
            "alias":"dbh",
            "formula":"(drc-0.002in)"

        },
        "pistoncylindergap":{
            "alias":"pcg",
            "formula":"(0.001 in*(drc-1 in))"

        },
        "newpistondiameter":{
            "alias":"npd",
            "formula":"(drc-pcg)"
        },
        "crankbearingrectification":{
            "alias":"cbr",
            "formula":"floor(cd-cp,2)+.01 in"

        },
        "crankbearingrectificationdiameter":{
            "alias":"crd",
            "formula":"cbr+cp"

        },
        "bearingjournalgap":{
            "alias":"bjg",
            "formula":"(CD*0.001in)*" 

        },
        "bearinginternaldiameter":{
            "alias":"bid",
            "formula":"(crd+bjg)"
        

        }
    }
}