{
    "inputParameters":{ 
        "diffRatio": {
                "value": null,
                "alias": "FFGR"
        },
        "maximumRPM": {
            "value": null,
            "alias": "nMax"
        },
        "gearCount":{
            "value":5,
            "alias":"gearCount"
        }
    }, 
    "formula_templates":{
        "gearGap": {
            "alias":"Gap_${n}_${n+1}",
            "formula":"nMax-(nMax*(GR_${n+1}/GR_${n}))"
        },
        "gearSpeed":{
            "alias":"Speed_${n}",
            "formula":"(nMax+(Tpr / 1000000 mm)*60 km/h)/(GR_${n}*FFGR) in km/h"
        }
    },
    "formulas":{
        "tirePerimeter":{
            "alias":"Tpr",
            "formula":"pi*TR*2"
        }
    }
}