{
    "inputParameters": {
        "AxleWeight": {
            "alias":"AW",
            "units":"kg",
            "value": null
       
        },
        "ArmShockDistance": {
            "alias":"D1",
            "units":"mm",
            "value": null
        },
        "lowerControlArmLenght":{
            "alias":"D2",
            "units":"mm",
            "value":null
        },

        "shockAngle": {
            "alias":"SA",
            "units":"rad",
            "value": null
        },
        "suspendedWeights":{
            "alias":"SW",
            "units":"kg",
            "value":null

        },
        "WireSpringDiameter": {
            "alias":"d",
            "units":"mm",
            "value": null
        },
        "outerSpringDiameter":{
            "alias":"D.Outer",
            "units":"mm",
            "value":null
        },

        "mediumSpringDiameter": {
            "alias":"D",
            "units":"mm",
            "value": null
        },
        "elasticityCoefficient":{
            "alias":"G",
            "units":"kg/mm^2",
            "value":null

        },
        "coilsNumber": {
            "alias":"n",
            "value": null
        },
        "pitchSpring":{
            "alias":"a",
            "units":"mm",
            "value":null

        },
        "radialDeformation":{
            "alias":"rD",
            "units":"m",
            "value":null

        }



    
    },
    "formulas":{
        "MotionRatio": {
            "alias":"MR",
            "formula":"D1/D2"
        },
        "CorrectionAngle":{
            "alias":"ACF",
            "formula":"cos (SA)"

        },
        "SpringConstant":{
            "alias":"K",
            "formula":"(d^4*G)/(8*n*D^3)"

        },
        "WheelRate":{
            "alias":"WR",
            "formula":"MR^2*K*ACF"

        },
        "RequiredForce":{
            "alias":"FR",
            "formula":"(AW/2)/MR"

        },
        "AdjustedForce":{
            "alias":"ASF",
            "formula":"FR/ACF"

        },

        "tireStifness":{
            "alias":"kn",
            "formula":"(((AW/2*9.81 m/s^2))/(rD)) in N/m"

        },
        "SuspensionStiffness":{
            "alias":"Tf",
            "formula":"((kn*1s^2/kg)*WR/1000)/(((kn*1s^2/kg)*1kg/m)+WR/1000) * 1kg/mm"

        },
        "Deformationunderload":{
            "alias":"dF",
            "formula":"(n*ASF*((D/2)^3)*64)/(G*(d^4))"

        },
        "Freelenght":{
            "alias":"L",
            "formula":"(n*(d+a)+dF in mm)"

        },

        "NoSuspendedWeightFrequency":{
            "alias":"Wm1",
            "formula":"(1/(2*pi))*sqrt(((kn*s^2/kg)+((K in kg/mm) *1mm/kg*9810))/((AW/2kg)-SW/1kg))*1Hz"

        },
        "SuspendenWeightFrequency":{
            "alias":"Wm2",
            "formula":"(1/(2*pi))*sqrt(((K*9810m/s^2)*kn)/((kn+(K*9810m/s^2))*(SW/1kg)))*(1s/kg^0.5)*1Hz"


        }
    }
}





    

